<?xml version='1.0' encoding='utf-8'?>
<job-scheduling-data>

  <schedule>

    <job>
      <name>ValuerecommenderReindexQueueProcessor</name>
      <job-class>org.metadatacenter.worker.ValuerecommenderReindexQueueProcessor</job-class>
      <concurrency-allowed>false</concurrency-allowed>
